pct.res <- function (data.frame, i.position, n.column, out.labels) {
pct = n.labels = c()
n = 1
for (i in  1:n.column) {
if(sum(data.frame[i+i.position]) > 0) {
pct[n] <- round(sum(data.frame[i+i.position])/nrow(data.frame)*100,1)
n.labels[n] <- out.labels[i]
n = n + 1}
}
names(pct) <- n.labels
return(pct)
}
